{
  "name": "create-next-example-app",
  "scripts": {
    "node-sass-watch": "./node_modules/node-sass/bin/node-sass -w scss/bundles/*.scss -o scss/bundles",
    "postcss-watch": "./node_modules/postcss-cli/bin/postcss scss/bundles/*.css --dir static/css -x css --watch",
    "build-sass": "./node_modules/node-sass/bin/node-sass scss/bundles/*.scss -o scss/bundles && ./node_modules/postcss-cli/bin/postcss scss/bundles/*.css --dir static/css -x css",
    "watch-sass": "run-p node-sass-watch postcss-watch",
    "start-dev": "node server",
    "dev": "run-p start-dev watch-sass",
    "build-production": "next build",
    "build": "run-s build-sass build-production",
    "start-production": "NODE_ENV=production node server",
    "start": "run-s build-sass start-production"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "express": "^4.16.2",
    "next": "^4.1.0",
    "next-redux-saga": "^1.0.1",
    "next-redux-wrapper": "^1.3.4",
    "next-routes": "^1.1.0",
    "nprogress": "^0.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "universal-cookie-express": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "cssnano": "^3.10.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.14",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "styled-jsx-css-loader": "^0.3.0"
  }
}
