{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "commonjs"
        }
      }
    ]
  ],
  "plugins": [
    "transform-flow-strip-types",
    [
      "module-resolver",
      { "root": ["./"] }
    ],
    [
      "import-static-files",
      {
        "extensions": [".jpg", ".png", ".jpeg", ".svg"] ,
        "baseDir": "/static/imported"
      }
    ]
  ]
}
